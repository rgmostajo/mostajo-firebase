{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/prop-types": "off",
        "no-undef": "off",
        "no-param-reassign": 0,
        "import/extensions": "off",
        "import/no-unresolved": "off",
        "react/destructuring-assignment": ["off", "always"],
        "no-console": "off"
    }
}
